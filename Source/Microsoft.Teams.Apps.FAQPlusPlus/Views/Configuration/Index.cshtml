@{
    ViewData["Title"] = "Configuration";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script>
    //initialize microsoftTeams in _Layout page
    microsoftTeams.settings.setValidityState(true);

    microsoftTeams.settings.registerOnSaveHandler((saveEvent) => {
        microsoftTeams.settings.setSettings({
            websiteUrl: "https://dev-dolphin.azurewebsites.net",
            contentUrl: "https://dev-dolphin.azurewebsites.net/content?theme={theme}",
            entityId: "dolphin-devTab",
            suggestedDisplayName: "Dolphin-Dev",
            removeUrl: ""
        });
        saveEvent.notifySuccess();
    });
</script>

<p>
    <h2 style="color:#6364a5">Configure your tab:</h2>
    <br />
    <br />
</p>
<H1>Add Dolphin to Channel Tab</H1>