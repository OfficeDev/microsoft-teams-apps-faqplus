{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.js",
        "jquery.min.map"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/teams-js@1.4.1",
      "destination": "wwwroot/lib/microsoft-teams/",
      "files": [
        "dist/MicrosoftTeams.min.js",
        "dist/MicrosoftTeams.js",
        "dist/MicrosoftTeams.js.map"
      ]
    },
    {
      "library": "i18next@21.9.0",
      "destination": "wwwroot/lib/i18next/"
    }
  ]
}